{
  "name": "11ty-starter-pack",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "serve": "npm run clear && npm run build:ts && run-p watch:* browser-sync",
    "browser-sync": "browser-sync start --config browser-sync.config.js",
    "watch:eleventy": "npx @11ty/eleventy --watch",
    "watch:ts": "rollup -c --watch",
    "build:ts": "rollup -c",
    "lint:scripts": "eslint '**/*.{js,ts}' --quiet --fix",
    "clear": "rimraf dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "browser-sync": "^2.26.13",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rollup": "^2.34.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  }
}
